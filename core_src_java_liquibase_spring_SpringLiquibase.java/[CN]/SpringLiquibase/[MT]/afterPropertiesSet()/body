{
  Connection c=null;
  try {
    c=getDataSource().getConnection();
    Liquibase liquibase=createLiquibase(c);
    liquibase.update(getContexts());
  }
 catch (  SQLException e) {
    throw new JDBCException(e);
  }
 finally {
    if (c != null) {
      try {
        c.rollback();
        c.close();
      }
 catch (      SQLException e) {
      }
    }
  }
}
