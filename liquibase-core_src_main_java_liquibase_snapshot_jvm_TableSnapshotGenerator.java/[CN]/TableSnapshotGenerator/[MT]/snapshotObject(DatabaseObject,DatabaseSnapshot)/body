{
  Database database=snapshot.getDatabase();
  String objectName=example.getName();
  Schema schema=example.getSchema();
  List<CachedRow> rs=null;
  try {
    JdbcDatabaseSnapshot.CachingDatabaseMetaData metaData=((JdbcDatabaseSnapshot)snapshot).getMetaData();
    rs=metaData.getTables(((AbstractJdbcDatabase)database).getJdbcCatalogName(schema),((AbstractJdbcDatabase)database).getJdbcSchemaName(schema),objectName,new String[]{"TABLE"});
    Table table;
    if (rs.size() > 0) {
      table=readTable(rs.get(0),database);
    }
 else {
      return null;
    }
    return table;
  }
 catch (  SQLException e) {
    throw new DatabaseException(e);
  }
}
