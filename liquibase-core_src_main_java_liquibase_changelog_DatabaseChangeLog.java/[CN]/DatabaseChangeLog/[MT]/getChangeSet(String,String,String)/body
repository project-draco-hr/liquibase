{
  for (  ChangeSet changeSet : changeSets) {
    if (changeSet.getFilePath().equalsIgnoreCase(path) && changeSet.getAuthor().equalsIgnoreCase(author) && changeSet.getId().equalsIgnoreCase(id)&& (changeSet.getDbmsSet() == null || changeLogParameters == null || changeLogParameters.getValue("database.typeName") == null || changeSet.getDbmsSet().isEmpty() || changeSet.getDbmsSet().contains(changeLogParameters.getValue("database.typeName").toString()))) {
      return changeSet;
    }
  }
  return null;
}
