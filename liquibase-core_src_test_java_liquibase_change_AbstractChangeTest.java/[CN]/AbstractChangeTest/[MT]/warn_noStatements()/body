{
  assertFalse(new ExampleAbstractChange(){
    @Override public SqlStatement[] generateStatements(    Database database){
      return null;
    }
  }
.warn(mock(Database.class)).hasWarnings());
  assertFalse(new ExampleAbstractChange(){
    @Override public SqlStatement[] generateStatements(    Database database){
      return new SqlStatement[0];
    }
  }
.warn(mock(Database.class)).hasWarnings());
}
