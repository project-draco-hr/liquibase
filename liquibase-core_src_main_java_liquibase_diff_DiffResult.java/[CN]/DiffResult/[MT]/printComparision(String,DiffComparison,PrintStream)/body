{
  out.print(title + ":");
  if (comparison == null) {
    out.print("NULL");
    return;
  }
  if (comparison.areTheSame()) {
    out.println(" EQUAL");
  }
 else {
    out.println();
    out.println("     Reference:   '" + comparison.getReferenceVersion() + "'");
    out.println("     Target: '" + comparison.getTargetVersion() + "'");
  }
}
