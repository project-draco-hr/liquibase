{
  String product;
  boolean flag=true;
  try {
    product=migrator.getDatabase().getConnection().getMetaData().getDatabaseProductName();
    if (type.equals(product.toLowerCase()))     flag=true;
 else     flag=false;
  }
 catch (  SQLException e) {
    throw new RuntimeException("Cannot determine database type");
  }
  return flag;
}
