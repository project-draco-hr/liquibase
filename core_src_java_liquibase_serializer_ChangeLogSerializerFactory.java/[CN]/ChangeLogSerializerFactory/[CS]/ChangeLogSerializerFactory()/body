{
  Class[] classes;
  try {
    classes=ClassPathScanner.getInstance().getClasses(ChangeLogSerializer.class);
    for (    Class clazz : classes) {
      register((ChangeLogSerializer)clazz.getConstructor().newInstance());
    }
  }
 catch (  Exception e) {
    throw new UnexpectedLiquibaseException(e);
  }
}
