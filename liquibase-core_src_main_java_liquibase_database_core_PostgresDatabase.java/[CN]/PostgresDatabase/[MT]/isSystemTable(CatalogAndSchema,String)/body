{
  schema=correctSchema(schema);
  return super.isSystemTable(schema,tableName) || "pg_catalog".equals(schema.getSchemaName()) || "pg_toast".equals(schema.getSchemaName())|| tableName.endsWith("_seq")|| tableName.endsWith("_key")|| tableName.endsWith("_pkey")|| tableName.startsWith("idx_")|| tableName.startsWith("pk_");
}
