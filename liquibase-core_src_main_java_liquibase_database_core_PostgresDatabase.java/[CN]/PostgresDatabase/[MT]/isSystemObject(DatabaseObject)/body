{
  if (example instanceof Table) {
    if ("pg_catalog".equals(example.getSchema().getName()) || "pg_toast".equals(example.getSchema().getName()) || example.getName().endsWith("_seq")|| example.getName().endsWith("_key")|| example.getName().endsWith("_pkey")|| example.getName().startsWith("idx_")|| example.getName().startsWith("pk_")) {
      return true;
    }
  }
  return super.isSystemObject(example);
}
