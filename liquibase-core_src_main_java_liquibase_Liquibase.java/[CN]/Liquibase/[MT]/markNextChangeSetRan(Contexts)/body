{
  changeLogParameters.setContexts(contexts);
  LockService lockService=LockServiceFactory.getInstance().getLockService(database);
  lockService.waitForLock();
  try {
    DatabaseChangeLog changeLog=ChangeLogParserFactory.getInstance().getParser(changeLogFile,resourceAccessor).parse(changeLogFile,changeLogParameters,resourceAccessor);
    checkLiquibaseTables(false,changeLog,contexts);
    changeLog.validate(database,contexts);
    ChangeLogIterator logIterator=new ChangeLogIterator(changeLog,new NotRanChangeSetFilter(database.getRanChangeSetList()),new ContextChangeSetFilter(contexts),new DbmsChangeSetFilter(database),new CountChangeSetFilter(1));
    logIterator.run(new ChangeLogSyncVisitor(database),database);
  }
  finally {
    lockService.releaseLock();
  }
}
