{
  context.getContext(ChangeLogParserContext.class).setSupportPropertyEscaping(true);
  this.handler=new ChangeLogParameters.ExpressionExpander(changeLogParameters,context);
  assertEquals("${user.name} and ${user.name} are literals but this isn't: " + System.getProperty("user.name"),handler.expandExpressions("${:user.name} and ${:user.name} are literals but this isn't: ${user.name}"));
}
