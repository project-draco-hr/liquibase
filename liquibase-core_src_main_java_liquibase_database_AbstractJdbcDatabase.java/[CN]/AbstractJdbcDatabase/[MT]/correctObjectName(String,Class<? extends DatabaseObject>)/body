{
  if (quotingStrategy == ObjectQuotingStrategy.QUOTE_ALL_OBJECTS || unquotedObjectsAreUppercased == null || objectName == null || (objectName.startsWith("\"") && objectName.endsWith("\""))) {
    return objectName;
  }
 else   if (unquotedObjectsAreUppercased == Boolean.TRUE) {
    return objectName.toUpperCase();
  }
 else {
    return objectName.toLowerCase();
  }
}
