{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  ForeignKey that=(ForeignKey)o;
  if (getForeignKeyColumns() == null) {
    return this.getName().equalsIgnoreCase(that.getName());
  }
  return (getForeignKeyColumns() != null && that.getForeignKeyColumns() != null && getForeignKeyColumns().equalsIgnoreCase(that.getForeignKeyColumns())) && (getForeignKeyTable() != null && that.getForeignKeyTable() != null && getForeignKeyTable().equals(that.getForeignKeyTable())) && (getPrimaryKeyColumns() != null && that.getPrimaryKeyColumns() != null && getPrimaryKeyColumns().equalsIgnoreCase(that.getPrimaryKeyColumns()))&& (getPrimaryKeyTable() != null && that.getPrimaryKeyTable() != null && getPrimaryKeyTable().equals(that.getPrimaryKeyTable()));
}
