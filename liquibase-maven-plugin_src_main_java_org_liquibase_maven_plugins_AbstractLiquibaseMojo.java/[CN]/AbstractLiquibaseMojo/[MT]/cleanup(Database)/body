{
  if (getLiquibase() != null) {
    try {
      getLiquibase().forceReleaseLocks();
    }
 catch (    LiquibaseException e) {
      getLog().error(e.getMessage(),e);
    }
  }
  if (db != null) {
    try {
      db.rollback();
      db.close();
    }
 catch (    DatabaseException e) {
      getLog().error("Failed to close open connection to database.",e);
    }
  }
}
