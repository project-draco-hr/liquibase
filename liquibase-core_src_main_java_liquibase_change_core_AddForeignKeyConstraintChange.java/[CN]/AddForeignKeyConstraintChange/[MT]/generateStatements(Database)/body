{
  boolean deferrable=false;
  if (getDeferrable() != null) {
    deferrable=getDeferrable();
  }
  boolean initiallyDeferred=false;
  if (getInitiallyDeferred() != null) {
    initiallyDeferred=getInitiallyDeferred();
  }
  return new SqlStatement[]{new AddForeignKeyConstraintStatement(getConstraintName(),getBaseTableCatalogName(),getBaseTableSchemaName(),getBaseTableName(),getBaseColumnNames(),getReferencedTableCatalogName(),getReferencedTableSchemaName(),getReferencedTableName(),getReferencedColumnNames()).setDeferrable(deferrable).setInitiallyDeferred(initiallyDeferred).setOnUpdate(getOnUpdate()).setOnDelete(getOnDelete()).setReferencesUniqueColumn(getReferencesUniqueColumn())};
}
