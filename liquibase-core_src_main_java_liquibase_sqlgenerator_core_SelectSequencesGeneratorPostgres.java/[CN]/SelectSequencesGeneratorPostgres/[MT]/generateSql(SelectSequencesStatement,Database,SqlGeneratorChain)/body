{
  String schema=statement.getSchemaName();
  return new Sql[]{new UnparsedSql("SELECT relname AS SEQUENCE_NAME FROM pg_class, pg_namespace " + "WHERE relkind='S' " + "AND pg_class.relnamespace = pg_namespace.oid "+ "AND nspname = '" + database.correctSchemaName(schema) + "' "+ "AND 'nextval(''"+ (schema == null ? "" : schema + ".")+ "'||relname||'''::regclass)' not in (select adsrc from pg_attrdef where adsrc is not null) "+ "AND 'nextval(''"+ (schema == null ? "" : schema + ".")+ "\"'||relname||'\"''::regclass)' not in (select adsrc from pg_attrdef where adsrc is not null) "+ "AND 'nextval('''||relname||'''::regclass)' not in (select adsrc from pg_attrdef where adsrc is not null)")};
}
