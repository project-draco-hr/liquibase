{
  RawSQLChange change=new RawSQLChange();
  assertEquals("sql:[\n" + "    splitStatements=\"true\"\n" + "    stripComments=\"false\"\n]",new StringChangeLogSerializer().serialize(change));
  change.setSql("some SQL Here");
  assertEquals("sql:[\n" + "    splitStatements=\"true\"\n" + "    sql=\"some SQL Here\"\n"+ "    stripComments=\"false\"\n"+ "]",new StringChangeLogSerializer().serialize(change));
}
