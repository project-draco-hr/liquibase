{
  SQLFileChange change=new SQLFileChange();
  assertEquals("sqlFile:[\n" + "    splitStatements=\"true\"\n" + "    stripComments=\"false\"\n]",new StringChangeLogSerializer().serialize(change,false));
  change.setPath("PATH/TO/File.txt");
  assertEquals("sqlFile:[\n" + "    path=\"PATH/TO/File.txt\"\n" + "    splitStatements=\"true\"\n"+ "    stripComments=\"false\"\n"+ "]",new StringChangeLogSerializer().serialize(change,false));
}
