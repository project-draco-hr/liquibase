{
  try {
    @SuppressWarnings("unused") DatabaseChangeLog changeLog=new XMLChangeLogSAXParser().parse("liquibase/changelog/parser/xml/missingChangeLog.xml",new HashMap<String,Object>(),new JUnitResourceAccessor());
