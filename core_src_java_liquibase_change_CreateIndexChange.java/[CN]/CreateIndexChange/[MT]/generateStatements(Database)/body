{
  List<String> columns=new ArrayList<String>();
  for (  ColumnConfig column : getColumns()) {
    columns.add(column.getName());
  }
  return new SqlStatement[]{new CreateIndexStatement(getIndexName(),getSchemaName() == null ? database.getDefaultSchemaName() : getSchemaName(),getTableName(),columns.toArray(new String[getColumns().size()])).setTablespace(getTablespace())};
}
