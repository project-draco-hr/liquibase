{
  String returnString=getName();
  if (parameters != null && parameters.size() > 0) {
    returnString+="(";
    for (    Object param : parameters) {
      if (returnString == null) {
        returnString+="NULL,";
      }
      returnString+=param.toString() + ",";
    }
    returnString=returnString.replaceFirst(",$","");
    returnString+=")";
  }
  return returnString.trim();
}
