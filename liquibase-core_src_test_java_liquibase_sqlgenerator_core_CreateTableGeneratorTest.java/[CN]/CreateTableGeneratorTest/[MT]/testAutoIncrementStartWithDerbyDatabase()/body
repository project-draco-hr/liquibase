{
  for (  Database database : TestContext.getInstance().getAllDatabases()) {
    if (database instanceof DerbyDatabase) {
      CreateTableStatement statement=new CreateTableStatement(SCHEMA_NAME,TABLE_NAME);
      statement.addColumn(COLUMN_NAME1,TypeConverterFactory.getInstance().findTypeConverter(database).getDataType("BIGINT",true),new AutoIncrementConstraint(COLUMN_NAME1,BigInteger.ZERO,null));
      Sql[] generatedSql=this.generatorUnderTest.generateSql(statement,database,null);
      assertEquals("CREATE TABLE SCHEMA_NAME.TABLE_NAME (COLUMN1_NAME BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 0))",generatedSql[0].toSql());
    }
  }
}
