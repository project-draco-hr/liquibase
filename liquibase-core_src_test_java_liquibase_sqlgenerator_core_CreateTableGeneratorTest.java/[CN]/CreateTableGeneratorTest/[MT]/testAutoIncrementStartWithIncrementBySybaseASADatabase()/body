{
  for (  Database database : TestContext.getInstance().getAllDatabases()) {
    if (database instanceof SybaseASADatabase) {
      CreateTableStatement statement=new CreateTableStatement(CATALOG_NAME,SCHEMA_NAME,TABLE_NAME);
      statement.addColumn(COLUMN_NAME1,DataTypeFactory.getInstance().fromDescription("BIGINT{autoIncrement:true}"),new AutoIncrementConstraint(COLUMN_NAME1,BigInteger.valueOf(2),BigInteger.TEN));
      Sql[] generatedSql=this.generatorUnderTest.generateSql(statement,database,null);
      assertEquals("CREATE TABLE CATALOG_NAME.SCHEMA_NAME.TABLE_NAME (COLUMN1_NAME BIGINT DEFAULT AUTOINCREMENT NULL)",generatedSql[0].toSql());
    }
  }
}
