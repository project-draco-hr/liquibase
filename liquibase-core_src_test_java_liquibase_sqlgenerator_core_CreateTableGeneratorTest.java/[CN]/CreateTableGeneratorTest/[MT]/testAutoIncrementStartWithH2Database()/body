{
  for (  Database database : TestContext.getInstance().getAllDatabases()) {
    if (database instanceof H2Database) {
      CreateTableStatement statement=new CreateTableStatement(CATALOG_NAME,SCHEMA_NAME,TABLE_NAME);
      statement.addColumn(COLUMN_NAME1,DataTypeFactory.getInstance().fromDescription("BIGINT{autoIncrement:true}"),new AutoIncrementConstraint(COLUMN_NAME1,BigInteger.ZERO,null));
      Sql[] generatedSql=this.generatorUnderTest.generateSql(statement,database,null);
      assertEquals("CREATE TABLE CATALOG_NAME.SCHEMA_NAME.TABLE_NAME (COLUMN1_NAME BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 0))",generatedSql[0].toSql());
    }
  }
}
