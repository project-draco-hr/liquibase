{
  for (  Database database : TestContext.getInstance().getAllDatabases()) {
    if (database instanceof SybaseASADatabase) {
      CreateTableStatement statement=new CreateTableStatement(SCHEMA_NAME,TABLE_NAME);
      statement.addColumn(COLUMN_NAME1,TypeConverterFactory.getInstance().findTypeConverter(database).getDataType("BIGINT",true),new AutoIncrementConstraint(COLUMN_NAME1,BigInteger.valueOf(2),null));
      Sql[] generatedSql=this.generatorUnderTest.generateSql(statement,database,null);
      assertEquals("CREATE TABLE \"SCHEMA_NAME\".\"TABLE_NAME\" (\"COLUMN1_NAME\" BIGINT DEFAULT AUTOINCREMENT NULL)",generatedSql[0].toSql());
    }
  }
}
