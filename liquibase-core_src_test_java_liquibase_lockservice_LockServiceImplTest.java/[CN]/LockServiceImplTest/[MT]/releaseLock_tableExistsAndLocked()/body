{
  Database database=createMock(Database.class);
  Executor executor=createMock(Executor.class);
  expect(executor.update(isA(UnlockDatabaseChangeLogStatement.class))).andReturn(1);
  expect(database.hasDatabaseChangeLogLockTable()).andReturn(true);
  database.commit();
  expectLastCall().atLeastOnce();
  database.setCanCacheLiquibaseTableInfo(false);
  expectLastCall();
  executor.comment("Release Database Lock");
  expectLastCall().anyTimes();
  database.rollback();
  expectLastCall().anyTimes();
  replay(database);
  replay(executor);
  ExecutorService.getInstance().setExecutor(database,executor);
  LockServiceImpl service=(LockServiceImpl)LockServiceImpl.getInstance(database);
  service.releaseLock();
  verify(database);
}
