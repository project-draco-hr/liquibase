{
  for (  Database database : TestContext.getInstance().getAvailableDatabases()) {
    dropAndCreateTable(new CreateTableStatement(TABLE_NAME).addColumn(COLUMN_NAME,"int",new NotNullConstraint()).addColumn(COLUMN2_NAME,"int",new NotNullConstraint()),database);
    if (database.supportsSchemas()) {
      dropAndCreateTable(new CreateTableStatement(TestContext.ALT_SCHEMA,TABLE_NAME).addColumn(COLUMN_NAME,"int",new NotNullConstraint()).addColumn(COLUMN2_NAME,"int",new NotNullConstraint()),database);
    }
  }
}
