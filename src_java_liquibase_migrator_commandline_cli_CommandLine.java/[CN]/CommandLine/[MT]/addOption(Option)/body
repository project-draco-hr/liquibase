{
  hashcodeMap.put(new Integer(opt.hashCode()),opt);
  String key=opt.getOpt();
  if (" ".equals(key)) {
    key=opt.getLongOpt();
  }
  if (options.get(key) != null) {
    ((java.util.List)options.get(key)).add(opt);
  }
 else {
    options.put(key,new java.util.ArrayList());
    ((java.util.List)options.get(key)).add(opt);
  }
}
