{
  if (schemas != null) {
    List<Schema> schemaObjs=new ArrayList<Schema>();
    for (    String name : schemas.split(",")) {
      schemaObjs.add(new Schema(catalog,name));
    }
    liquibase.dropAll(schemaObjs.toArray(new Schema[schemaObjs.size()]));
  }
 else {
    liquibase.dropAll();
  }
}
