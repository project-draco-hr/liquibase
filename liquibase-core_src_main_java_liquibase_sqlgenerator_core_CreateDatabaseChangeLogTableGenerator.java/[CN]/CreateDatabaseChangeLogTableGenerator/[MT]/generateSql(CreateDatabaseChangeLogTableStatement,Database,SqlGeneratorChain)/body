{
  CreateTableStatement createTableStatement=new CreateTableStatement(database.getLiquibaseSchemaName(),database.getDatabaseChangeLogTableName()).addPrimaryKeyColumn("ID","VARCHAR(63)",null,null,new NotNullConstraint()).addPrimaryKeyColumn("AUTHOR","VARCHAR(63)",null,null,new NotNullConstraint()).addPrimaryKeyColumn("FILENAME","VARCHAR(200)",null,null,new NotNullConstraint()).addColumn("DATEEXECUTED",TypeConverterFactory.getInstance().findTypeConverter(database).getDateTimeType().getDataTypeName(),new NotNullConstraint()).addColumn("ORDEREXECUTED","INT",new NotNullConstraint(),new UniqueConstraint("UQ_DBCL_ORDEREXEC")).addColumn("MD5SUM","VARCHAR(35)").addColumn("DESCRIPTION","VARCHAR(255)").addColumn("COMMENTS","VARCHAR(255)").addColumn("TAG","VARCHAR(255)").addColumn("LIQUIBASE","VARCHAR(10)");
  if (database instanceof MSSQLDatabase) {
    createTableStatement.setSchemaName("dbo");
  }
  return SqlGeneratorFactory.getInstance().generateSql(createTableStatement,database);
}
