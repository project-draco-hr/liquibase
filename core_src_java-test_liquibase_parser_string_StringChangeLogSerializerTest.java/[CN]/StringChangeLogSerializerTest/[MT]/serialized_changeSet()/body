{
  ChangeSet changeSet=new ChangeSet("1","ted",true,false,"com/example/test.xml","c:/com/exmple/test","context1, context2","mysql, oracle");
  AddColumnChange change=new AddColumnChange();
  changeSet.addChange(change);
  assertEquals("changeSet:[\n" + "    alwaysRun=\"true\"\n" + "    author=\"test\"\n"+ "    contextList=\"context1,context2\"\n"+ "    dbmsList=\"mysql,oracle\"\n"+ "    filePath=\"com/example/test.xml\"\n"+ "    id=\"1\"\n"+ "    physicalFilePath=\"c:/com/example/test.xml\"\n"+ "    runOnChange=\"false\"\n"+ "    changes: [\n"+ "        addColumn:[\n"+ "            columns=[]\n"+ "        ]\n"+ "    ]\n"+ "]",new StringChangeLogSerializer().serialize(changeSet));
}
