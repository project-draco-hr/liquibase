{
  if (value == null) {
    return null;
  }
  String returnValue;
  TypeConverter converter=TypeConverterFactory.getInstance().findTypeConverter(database);
  if (((Boolean)value)) {
    returnValue=converter.getTrueBooleanValue();
  }
 else {
    returnValue=converter.getFalseBooleanValue();
  }
  if (returnValue.matches("\\d+")) {
    return returnValue;
  }
 else {
    return "'" + returnValue + "'";
  }
}
