  return ("CREATE TABLE DATABASECHANGELOG (id varchar(150) not null, " + "author varchar(150) not null, " + "filename varchar(255) not null, "+ "dateExecuted " + getDateTimeType() + " not null, "+ "md5sum varchar(32), "+ "description varchar(255), "+ "comments varchar(255), "+ "tag varchar(255), "+ "liquibase varchar(10), "+ "primary key(id, author, filename))").toUpperCase();
