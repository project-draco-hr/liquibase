{
  return new CreateTableStatement(null,getDatabaseChangeLogTableName()).addPrimaryKeyColumn("ID","VARCHAR(63)",new NotNullConstraint()).addPrimaryKeyColumn("AUTHOR","VARCHAR(63)",new NotNullConstraint()).addPrimaryKeyColumn("FILENAME","VARCHAR(255)",new NotNullConstraint()).addColumn("DATEEXECUTED",getDateTimeType(),new NotNullConstraint()).addColumn("MD5SUM","VARCHAR(32)").addColumn("DESCRIPTION","VARCHAR(255)").addColumn("COMMENTS","VARCHAR(255)").addColumn("TAG","VARCHAR(255)").addColumn("LIQUIBASE","VARCHAR(10)");
}
