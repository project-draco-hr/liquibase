{
  return new CreateTableStatement(getDefaultSchemaName(),getDatabaseChangeLogTableName()).addPrimaryKeyColumn("ID","VARCHAR(63)",null,new NotNullConstraint()).addPrimaryKeyColumn("AUTHOR","VARCHAR(63)",null,new NotNullConstraint()).addPrimaryKeyColumn("FILENAME","VARCHAR(200)",null,new NotNullConstraint()).addColumn("DATEEXECUTED",getDateTimeType(),new NotNullConstraint()).addColumn("MD5SUM","VARCHAR(32)").addColumn("DESCRIPTION","VARCHAR(255)").addColumn("COMMENTS","VARCHAR(255)").addColumn("TAG","VARCHAR(255)").addColumn("LIQUIBASE","VARCHAR(10)");
}
