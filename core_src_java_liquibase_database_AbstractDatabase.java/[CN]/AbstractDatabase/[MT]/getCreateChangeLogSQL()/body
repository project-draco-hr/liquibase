{
  return new CreateTableStatement(null,getDatabaseChangeLogTableName()).addPrimaryKeyColumn("id","varchar(150)",new NotNullConstraint()).addPrimaryKeyColumn("author","varchar(150)",new NotNullConstraint()).addPrimaryKeyColumn("filename","varchar(255)",new NotNullConstraint()).addColumn("dateExecuted",getDateTimeType(),new NotNullConstraint()).addColumn("md5sum","varchar(32)").addColumn("description","varchar(255)").addColumn("comments","varchar(255)").addColumn("tag","varchar(255)").addColumn("liquibase","varchar(10)");
}
