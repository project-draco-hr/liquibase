{
  if (super.isSystemTable(schema,tableName)) {
    return true;
  }
 else   if (tableName.startsWith("BIN$")) {
    return true;
  }
 else   if (tableName.startsWith("AQ$")) {
    return true;
  }
 else   if (tableName.startsWith("DR$")) {
    return true;
  }
 else   if (tableName.startsWith("SYS_IOT_OVER")) {
    return true;
  }
  return false;
}
