{
  try {
    StringBuffer result=new StringBuffer();
    char[] buffer=new char[2048];
    int read;
    while ((read=reader.read(buffer)) > -1) {
      result.append(buffer,0,read);
    }
    return result.toString();
  }
  finally {
    try {
      reader.close();
    }
 catch (    IOException ioe) {
    }
  }
}
