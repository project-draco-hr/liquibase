{
  setLogLevel(logLevel);
  if (logFile != null) {
    File log=new File(logFile);
    try {
      if (!log.exists()) {
        if (!log.createNewFile()) {
          throw new RuntimeException("Could not create logFile " + log.getAbsolutePath());
        }
      }
      err=new PrintStream(log);
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
