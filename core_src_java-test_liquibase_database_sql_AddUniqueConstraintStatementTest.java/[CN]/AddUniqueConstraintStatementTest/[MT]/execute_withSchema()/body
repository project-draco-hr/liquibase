{
  new DatabaseTestTemplate().testOnAvailableDatabases(new SqlStatementDatabaseTest(TestContext.ALT_SCHEMA,new AddUniqueConstraintStatement(TestContext.ALT_SCHEMA,TABLE_NAME,COLUMN_NAME,"uq_adduqtest")){
    protected void preExecuteAssert(    DatabaseSnapshot snapshot){
      assertFalse(snapshot.getTable(TABLE_NAME).getColumn(COLUMN_NAME).isUnique());
    }
