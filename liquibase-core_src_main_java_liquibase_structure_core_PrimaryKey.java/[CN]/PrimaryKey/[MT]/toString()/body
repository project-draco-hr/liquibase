{
  if (getTable() == null) {
    return getName();
  }
 else {
    String tableName=getTable().getName();
    if (getTable().getSchema() != null) {
      tableName=getTable().getSchema().toString() + "." + tableName;
    }
    return getName() + " on " + tableName+ "("+ getColumnNames()+ ")";
  }
}
