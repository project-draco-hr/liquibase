{
  if (columnNames == null) {
    return null;
  }
  List<String> columnNameList=StringUtils.splitAndTrim(columnNames,",");
  Column[] returnArray=new Column[columnNameList.size()];
  for (int i=0; i < columnNameList.size(); i++) {
    returnArray[i]=new Column(columnNameList.get(i));
  }
  return returnArray;
}
