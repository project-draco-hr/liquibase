{
  if (getRelation() == null) {
    return getName();
  }
 else {
    String tableOrViewName=getRelation().getName();
    if (getRelation().getSchema() != null && getRelation().getSchema().getName() != null) {
      tableOrViewName=getRelation().getSchema().getName() + "." + tableOrViewName;
    }
    return tableOrViewName + "." + getName();
  }
}
