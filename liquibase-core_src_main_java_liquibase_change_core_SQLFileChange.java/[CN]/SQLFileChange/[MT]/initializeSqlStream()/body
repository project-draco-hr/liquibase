{
  if (path == null) {
    return true;
  }
  try {
    sqlStream=StreamUtil.openStream(path,isRelativeToChangelogFile(),getChangeSet(),getResourceAccessor());
  }
 catch (  IOException e) {
    throw new IOException("<sqlfile path=" + path + "> -Unable to read file",e);
  }
  return sqlStream != null;
}
