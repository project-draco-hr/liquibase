{
  String sql=super.getSql();
  if (sql == null) {
    if (sqlStream == null) {
      return null;
    }
    try {
      setSql(StreamUtil.getStreamContents(sqlStream,encoding));
    }
 catch (    IOException e) {
      throw new UnexpectedLiquibaseException(e);
    }
    sql=super.getSql();
  }
  return sql;
}
