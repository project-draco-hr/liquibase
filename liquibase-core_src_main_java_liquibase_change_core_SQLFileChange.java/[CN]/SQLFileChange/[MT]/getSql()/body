{
  String sql=super.getSql();
  if (sql == null) {
    InputStream sqlStream;
    try {
      sqlStream=openSqlStream();
      if (sqlStream == null) {
        return null;
      }
      return StreamUtil.getStreamContents(sqlStream,encoding);
    }
 catch (    IOException e) {
      throw new UnexpectedLiquibaseException(e);
    }
  }
 else {
    return sql;
  }
}
