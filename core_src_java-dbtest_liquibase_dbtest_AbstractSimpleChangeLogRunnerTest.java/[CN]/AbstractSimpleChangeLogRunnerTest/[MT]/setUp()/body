{
  super.setUp();
  if (database != null) {
    if (!database.getConnection().getAutoCommit()) {
      database.rollback();
    }
    LockService.getInstance(database).forceReleaseLock();
    if (database.supportsSchemas()) {
      database.dropDatabaseObjects(TestContext.ALT_SCHEMA);
    }
    database.dropDatabaseObjects(null);
    database.commit();
  }
}
