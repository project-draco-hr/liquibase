{
  CountChangeSetFilter filter=new CountChangeSetFilter(2);
  assertTrue(filter.accepts(new ChangeSet("a1","b1",false,false,"c1",null,null)));
  assertTrue(filter.accepts(new ChangeSet("a2","b2",false,false,"c2",null,null)));
  assertFalse(filter.accepts(new ChangeSet("a3","b3",false,false,"c3",null,null)));
}
