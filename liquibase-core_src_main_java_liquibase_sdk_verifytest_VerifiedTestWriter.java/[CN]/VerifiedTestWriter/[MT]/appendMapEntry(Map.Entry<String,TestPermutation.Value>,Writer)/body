{
  String value=entry.getValue().serialize();
  value=value.replace("\r\n","\n");
  boolean multiLine=value.contains("\n");
  out.append("- **").append(entry.getKey());
  if (multiLine) {
    out.append(" =>**\n");
  }
 else {
    out.append(":** ");
  }
  if (multiLine) {
    out.append(StringUtils.indent(value,4));
  }
 else {
    out.append(value);
  }
  out.append("\n");
}
