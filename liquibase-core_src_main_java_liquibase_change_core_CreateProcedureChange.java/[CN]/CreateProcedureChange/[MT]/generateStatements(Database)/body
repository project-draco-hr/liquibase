{
  String endDelimiter=";";
  if (database instanceof OracleDatabase) {
    endDelimiter="\n/";
  }
 else   if (database instanceof DB2Database) {
    endDelimiter="";
  }
  return new SqlStatement[]{new CreateProcedureStatement(getCatalogName(),getSchemaName(),getProcedureName(),getProcedureText(),endDelimiter)};
}
