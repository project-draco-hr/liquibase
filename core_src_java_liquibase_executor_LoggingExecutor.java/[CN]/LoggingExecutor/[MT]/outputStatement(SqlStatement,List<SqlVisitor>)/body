{
  try {
    for (    String statement : applyVisitors(sql,sqlVisitors)) {
      output.write(statement);
      if (database instanceof MSSQLDatabase) {
        output.write(StreamUtil.getLineSeparator());
        output.write("GO");
      }
 else {
        if (!statement.endsWith(";")) {
          output.write(";");
        }
      }
      output.write(StreamUtil.getLineSeparator());
      output.write(StreamUtil.getLineSeparator());
    }
  }
 catch (  IOException e) {
    throw new JDBCException(e);
  }
}
