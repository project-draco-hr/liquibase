{
  this.type=name;
  String[] stringParams=new String[parameters.length];
  if (parameters.length > 0) {
    for (int i=0; i < parameters.length; i++) {
      if (parameters[i] == null) {
        stringParams[i]="NULL";
      }
 else {
        stringParams[i]=parameters[i].toString();
      }
    }
    type+="(" + StringUtils.join(stringParams,", ") + ")";
  }
}
