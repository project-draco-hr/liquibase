{
  for (  Database database : TestContext.getInstance().getAllDatabases()) {
    if (database instanceof H2Database || database instanceof HsqlDatabase) {
      super.testAddAutoIncrementStatement(database,BigInteger.valueOf(2),BigInteger.TEN,"ALTER TABLE " + database.escapeTableName("CATALOG_NAME","SCHEMA_NAME","TABLE_NAME") + " ALTER COLUMN COLUMN_NAME BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 2, INCREMENT BY 10)");
    }
  }
}
