{
  String type=super.getColumnType(columnType,autoIncrement);
  if (autoIncrement != null && autoIncrement) {
    if (isSerial(type)) {
      return "SERIAL";
    }
 else     if (isSerial8(type)) {
      return "SERIAL8";
    }
 else {
      throw new IllegalArgumentException("Unknown autoincrement type: " + columnType);
    }
  }
  return type;
}
