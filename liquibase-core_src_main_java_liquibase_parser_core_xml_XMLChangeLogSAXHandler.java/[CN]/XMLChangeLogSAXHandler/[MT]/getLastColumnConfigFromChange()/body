{
  T result=null;
  if (change instanceof ChangeWithColumns) {
    List<T> columns=((ChangeWithColumns)change).getColumns();
    if (columns.size() > 0) {
      result=columns.get(columns.size() - 1);
    }
  }
 else {
    throw new RuntimeException("Unexpected change: " + change.getClass().getName());
  }
  return result;
}
