{
  Statement st=null;
  try {
    st=createStatement();
    st.execute("CALL SYSCS_UTIL.SYSCS_CHECKPOINT_DATABASE()");
  }
 catch (  SQLException e) {
    throw new DatabaseException(e);
  }
 finally {
    if (st != null) {
      try {
        st.close();
      }
 catch (      SQLException e) {
        ;
      }
    }
  }
}
