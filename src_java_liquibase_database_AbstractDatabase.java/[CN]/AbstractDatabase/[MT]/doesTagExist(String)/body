{
  PreparedStatement pstmt=null;
  ResultSet rs=null;
  try {
    pstmt=getConnection().prepareStatement("SELECT COUNT(*) FROM " + getDatabaseChangeLogTableName() + " WHERE TAG=?");
    pstmt.setString(1,tag);
    rs=pstmt.executeQuery();
    rs.next();
    return rs.getInt(1) > 0;
  }
  finally {
    if (rs != null) {
      rs.close();
    }
    if (pstmt != null) {
      pstmt.close();
    }
  }
}
