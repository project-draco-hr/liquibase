{
  Connection conn=getConnection();
  try {
    conn.setAutoCommit(false);
    dropForeignKeys(conn);
    dropViews(conn);
    dropTables(conn);
    if (this.supportsSequences()) {
      dropSequences(conn);
    }
    changeLogTableExists=false;
  }
 catch (  SQLException e) {
    throw new JDBCException(e);
  }
 finally {
    try {
      conn.commit();
    }
 catch (    SQLException e) {
      throw new JDBCException(e);
    }
  }
}
