{
  if (resourceAccessor == null) {
    File samples1=new File(TestContext.getInstance().findCoreJvmProjectRoot(),"/lib-test/liquibase-sample1.jar");
    File samples2=new File(TestContext.getInstance().findCoreJvmProjectRoot(),"/lib-test/liquibase-sample2.jar");
    resourceAccessor=new CompositeResourceAccessor(new ClassLoaderResourceAccessor(),new ClassLoaderResourceAccessor(new URLClassLoader(new URL[]{samples1.toURL(),samples2.toURL(),new File(TestContext.getInstance().findCoreJvmProjectRoot(),"/build").toURL(),new File(TestContext.getInstance().findCoreProjectRoot(),"/build").toURL()})));
  }
  return resourceAccessor;
}
