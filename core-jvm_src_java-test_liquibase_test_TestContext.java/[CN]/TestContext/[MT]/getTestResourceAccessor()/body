{
  if (resourceAccessor == null) {
    File samples1=new File(TestContext.getInstance().findCoreJvmProjectRoot(),"/lib-test/liquibase-sample1.jar");
    File samples2=new File(TestContext.getInstance().findCoreJvmProjectRoot(),"/lib-test/liquibase-sample2.jar");
    if (!samples2.exists()) {
      throw new RuntimeException("Could not find " + samples2.getAbsolutePath());
    }
    resourceAccessor=new CompositeResourceAccessor(new ClassLoaderResourceAccessor(),new ClassLoaderResourceAccessor(new URLClassLoader(new URL[]{samples2.toURL(),new File(TestContext.getInstance().findCoreJvmProjectRoot(),"/build").toURL(),new File(TestContext.getInstance().findCoreProjectRoot(),"/build").toURL()})));
  }
  return resourceAccessor;
}
