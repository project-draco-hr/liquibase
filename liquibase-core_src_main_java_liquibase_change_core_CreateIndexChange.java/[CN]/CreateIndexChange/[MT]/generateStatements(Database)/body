{
  List<String> columns=new ArrayList<String>();
  for (  ColumnConfig column : getColumn()) {
    columns.add(column.getName());
  }
  return new SqlStatement[]{new CreateIndexStatement(getIndexName(),getCatalogName(),getSchemaName(),getTableName(),this.isUnique(),getAssociatedWith(),columns.toArray(new String[getColumn().size()])).setTablespace(getTablespace())};
}
