{
  String sql="SELECT LOCKED FROM " + database.escapeTableName(database.getDefaultSchemaName(),database.getDatabaseChangeLogLockTableName()) + " WHERE "+ database.escapeColumnName(database.getDefaultSchemaName(),database.getDatabaseChangeLogLockTableName(),"ID")+ "=1";
  if (database instanceof OracleDatabase) {
    sql+=" for update";
  }
  return new Sql[]{new UnparsedSql(sql)};
}
