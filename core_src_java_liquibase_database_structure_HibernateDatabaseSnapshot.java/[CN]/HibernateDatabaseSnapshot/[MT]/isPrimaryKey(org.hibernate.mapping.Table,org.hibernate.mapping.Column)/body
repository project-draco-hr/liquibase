{
  org.hibernate.mapping.PrimaryKey key=hibernateTable.getPrimaryKey();
  if (key == null) {
    return false;
  }
  Iterator columnIterator=key.getColumnIterator();
  while (columnIterator.hasNext()) {
    if (columnIterator.next().equals(hibernateColumn)) {
      return true;
    }
  }
  return false;
}
