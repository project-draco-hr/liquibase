{
  assertTrue(generatorUnderTest.validate(new AddAutoIncrementStatement(null,null,null,null,null,null,null),new InformixDatabase(),new MockSqlGeneratorChain()).getErrorMessages().contains("columnDataType is required"));
  assertFalse(generatorUnderTest.validate(new AddAutoIncrementStatement(null,null,"table_name","column_name","int",null,null),new InformixDatabase(),new MockSqlGeneratorChain()).hasErrors());
}
