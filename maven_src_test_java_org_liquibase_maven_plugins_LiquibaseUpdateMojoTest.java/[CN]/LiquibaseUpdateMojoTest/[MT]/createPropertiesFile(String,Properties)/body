{
  File output=new File(getBasedir(),"/target/test-classes/" + filename);
  if (!output.exists()) {
    System.out.println("Creating file: " + output.getPath());
    if (!output.createNewFile()) {
      throw new IOException("Unable to create the properties file, " + output.getAbsolutePath());
    }
  }
  FileWriter fw=null;
  try {
    fw=new FileWriter(output);
    p.store(fw,"generated by unit test");
  }
  finally {
    if (fw != null) {
      fw.close();
    }
  }
}
