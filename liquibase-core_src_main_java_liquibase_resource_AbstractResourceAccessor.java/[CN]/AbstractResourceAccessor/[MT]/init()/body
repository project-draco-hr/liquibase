{
  try {
    Enumeration<URL> baseUrls;
    ClassLoader classLoader=toClassLoader();
    if (classLoader != null) {
      baseUrls=classLoader.getResources("");
      while (baseUrls.hasMoreElements()) {
        addRootPath(baseUrls.nextElement());
      }
    }
  }
 catch (  IOException e) {
    throw new UnexpectedLiquibaseException(e);
  }
}
