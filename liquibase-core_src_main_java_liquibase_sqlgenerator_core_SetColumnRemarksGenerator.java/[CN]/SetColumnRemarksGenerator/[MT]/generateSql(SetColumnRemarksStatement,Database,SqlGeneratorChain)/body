{
  return new Sql[]{new UnparsedSql("COMMENT ON COLUMN " + database.escapeTableName(statement.getCatalogName(),statement.getSchemaName(),statement.getTableName()) + "."+ database.escapeColumnName(statement.getCatalogName(),statement.getSchemaName(),statement.getTableName(),statement.getColumnName())+ " IS '"+ database.escapeStringForDatabase(StringUtils.trimToEmpty(statement.getRemarks()))+ "'",getAffectedColumn(statement))};
}
