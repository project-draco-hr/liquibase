{
  int prefix=getPrefixLength(fullFilenameToAdd);
  if (prefix < 0) {
    return null;
  }
  if (prefix > 0) {
    return normalize(fullFilenameToAdd);
  }
  if (basePath == null) {
    return null;
  }
  int len=basePath.length();
  if (len == 0) {
    return normalize(fullFilenameToAdd);
  }
  char ch=basePath.charAt(len - 1);
  if (isSeparator(ch)) {
    return normalize(basePath + fullFilenameToAdd);
  }
 else {
    return normalize(basePath + '/' + fullFilenameToAdd);
  }
}
