{
  if (otherObject == null) {
    return false;
  }
  if (this.getName() == null) {
    return otherObject.getName() == null;
  }
  if (accordingTo.isCaseSensitive()) {
    return this.getName().equals(otherObject.getName());
  }
 else {
    return this.getName().equalsIgnoreCase(otherObject.getName());
  }
}
