{
  while (iter.hasNext()) {
    String var=(String)iter.next();
    if (options.hasOption(var)) {
      iter.previous();
      break;
    }
 else     if (!opt.addValue(var)) {
      iter.previous();
      break;
    }
  }
  if (opt.getValues() == null && !opt.hasOptionalArg()) {
    throw new MissingArgumentException("no argument for:" + opt.getOpt());
  }
}
