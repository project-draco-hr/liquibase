{
  List<Change> changes=getChanges();
  if (changes.size() == 0) {
    return "Empty";
  }
  SortedSet<String> messages=new TreeSet<String>();
  for (  Change change : changes) {
    messages.add(change.getConfirmationMessage());
  }
  return StringUtils.limitSize(StringUtils.join(messages,","),255);
}
