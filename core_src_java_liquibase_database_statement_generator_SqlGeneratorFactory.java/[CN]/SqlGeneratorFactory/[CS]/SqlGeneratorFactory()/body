{
  Class[] classes;
  try {
    classes=PluginUtil.getClasses("liquibase.database.statement.generator",SqlGenerator.class);
    for (    Class clazz : classes) {
      register((SqlGenerator)clazz.getConstructor().newInstance());
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
