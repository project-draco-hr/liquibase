{
  for (  PrimaryKey pk : getPrimaryKeys()) {
    if (columnInfo.getTable() == null) {
      continue;
    }
    if (pk.getTable().getName().equalsIgnoreCase(columnInfo.getTable().getName())) {
      if (pk.getColumnNamesAsList().contains(columnInfo.getName())) {
        return true;
      }
    }
  }
  return false;
}
