{
  InputStream stream=null;
  try {
    stream=getFileOpener().getResourceAsStream(getFile());
    if (stream == null) {
      throw new RuntimeException(getFile() + " could not be found");
    }
    return CheckSum.compute(stream);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    if (stream != null) {
      try {
        stream.close();
      }
 catch (      IOException e) {
        ;
      }
    }
  }
}
