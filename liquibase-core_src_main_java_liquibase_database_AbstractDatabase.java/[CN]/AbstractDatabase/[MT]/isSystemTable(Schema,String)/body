{
  schema=correctSchema(schema);
  if ("information_schema".equalsIgnoreCase(schema.getName())) {
    return true;
  }
 else   if (tableName.equalsIgnoreCase(getDatabaseChangeLogLockTableName())) {
    return true;
  }
 else   if (getSystemTablesAndViews().contains(tableName)) {
    return true;
  }
  return false;
}
