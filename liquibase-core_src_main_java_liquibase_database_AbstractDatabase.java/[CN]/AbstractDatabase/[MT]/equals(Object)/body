{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  AbstractDatabase that=(AbstractDatabase)o;
  if (connection == null) {
    if (that.connection == null) {
      return this == that;
    }
 else {
      return false;
    }
  }
 else {
    return connection.equals(that.connection);
  }
}
