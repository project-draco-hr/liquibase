{
  String sql="ALTER TABLE " + database.escapeTableName(statement.getCatalogName(),statement.getSchemaName(),statement.getTableName()) + " MODIFY "+ database.escapeColumnName(statement.getCatalogName(),statement.getSchemaName(),statement.getTableName(),statement.getColumnName())+ " "+ DataTypeFactory.getInstance().fromDescription(statement.getColumnDataType() + "{autoIncrement:true}").toDatabaseDataType(database)+ " "+ database.getAutoIncrementClause(statement.getStartWith(),statement.getIncrementBy());
  return new Sql[]{new UnparsedSql(sql,getAffectedColumn(statement))};
}
