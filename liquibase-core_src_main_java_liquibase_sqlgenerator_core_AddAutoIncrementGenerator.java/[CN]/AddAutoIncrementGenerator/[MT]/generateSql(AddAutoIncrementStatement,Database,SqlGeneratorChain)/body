{
  String sql="ALTER TABLE " + database.escapeTableName(statement.getSchemaName(),statement.getTableName()) + " MODIFY "+ database.escapeColumnName(statement.getSchemaName(),statement.getTableName(),statement.getColumnName())+ " "+ TypeConverterFactory.getInstance().findTypeConverter(database).getDataType(statement.getColumnDataType(),true)+ " AUTO_INCREMENT";
  return new Sql[]{new UnparsedSql(sql,new Column().setTable(new Table(statement.getTableName()).setSchema(statement.getSchemaName())).setName(statement.getColumnName()))};
}
