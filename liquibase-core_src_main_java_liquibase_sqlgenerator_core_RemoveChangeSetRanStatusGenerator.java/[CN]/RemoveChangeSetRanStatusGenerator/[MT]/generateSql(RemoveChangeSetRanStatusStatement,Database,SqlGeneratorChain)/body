{
  ChangeSet changeSet=statement.getChangeSet();
  return SqlGeneratorFactory.getInstance().generateSql(new DeleteStatement(database.getLiquibaseCatalogName(),database.getLiquibaseSchemaName(),database.getDatabaseChangeLogTableName()).setWhereClause("ID=? AND AUTHOR=? AND FILENAME=?").addWhereParameters(changeSet.getId(),changeSet.getAuthor(),changeSet.getFilePath()),database);
}
