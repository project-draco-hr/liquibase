{
  List<List<String>> cells=new ArrayList<List<String>>();
  for (  Column column : table.getColumns()) {
    String remarks=column.getRemarks();
    cells.add(Arrays.asList(column.getType().toString(),"<A HREF=\"../columns/" + table.getName().toLowerCase() + "."+ column.getName().toLowerCase()+ ".html"+ "\">"+ column.getName()+ "</A>",remarks != null ? remarks : ""));
  }
  writeTable("Current Columns",cells,fileWriter);
}
