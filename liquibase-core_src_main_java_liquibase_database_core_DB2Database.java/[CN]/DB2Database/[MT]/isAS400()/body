{
  if (this.isAS400 == null) {
    if (getConnection() != null && getConnection() instanceof JdbcConnection) {
      try {
        this.isAS400=getConnection().getDatabaseProductName().startsWith("DB2 UDB for AS/400");
      }
 catch (      DatabaseException e) {
        this.isAS400=false;
      }
    }
 else {
      this.isAS400=false;
    }
  }
  return this.isAS400;
}
