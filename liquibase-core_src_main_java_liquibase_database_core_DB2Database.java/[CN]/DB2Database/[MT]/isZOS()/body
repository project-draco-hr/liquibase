{
  if (this.isZOS == null) {
    if (getConnection() != null && getConnection() instanceof JdbcConnection) {
      try {
        this.isZOS=getConnection().getDatabaseProductName().toLowerCase().contains("zos");
      }
 catch (      DatabaseException e) {
        this.isZOS=false;
      }
    }
 else {
      this.isZOS=false;
    }
  }
  return this.isZOS;
}
