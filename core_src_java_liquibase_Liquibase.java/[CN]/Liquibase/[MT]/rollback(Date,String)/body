{
  LockService lockService=LockService.getInstance(database);
  lockService.waitForLock();
  try {
    database.checkDatabaseChangeLogTable();
    DatabaseChangeLog changeLog=ChangeLogParserFactory.getInstance().getParser(changeLogFile).parse(changeLogFile,changeLogParameters,fileOpener);
    changeLog.validate(database);
    ChangeLogIterator logIterator=new ChangeLogIterator(changeLog,new ExecutedAfterChangeSetFilter(dateToRollBackTo,database.getRanChangeSetList()),new ContextChangeSetFilter(contexts),new DbmsChangeSetFilter(database));
    logIterator.run(new RollbackVisitor(database),database);
  }
  finally {
    lockService.releaseLock();
  }
}
