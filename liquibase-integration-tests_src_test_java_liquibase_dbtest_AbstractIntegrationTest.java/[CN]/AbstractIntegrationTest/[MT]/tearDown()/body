{
  if (database != null) {
    if (shouldRollBack()) {
      database.rollback();
    }
    ExecutorService.getInstance().clearExecutor(database);
    database.setDefaultSchemaName(null);
    database.setOutputDefaultCatalog(false);
    database.setOutputDefaultSchema(false);
  }
  SnapshotGeneratorFactory.resetAll();
}
