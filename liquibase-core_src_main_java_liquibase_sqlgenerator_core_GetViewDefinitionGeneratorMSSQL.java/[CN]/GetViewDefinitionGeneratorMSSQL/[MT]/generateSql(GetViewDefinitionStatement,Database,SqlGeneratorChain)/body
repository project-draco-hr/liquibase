{
  try {
    String sql="exec sp_helptext '" + database.convertRequestedSchemaToSchema(statement.getSchemaName()) + "."+ statement.getViewName().toUpperCase()+ "'";
    return new Sql[]{new UnparsedSql(sql)};
  }
 catch (  DatabaseException e) {
    throw new UnexpectedLiquibaseException(e);
  }
}
